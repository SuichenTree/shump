<view class="exam">
  <scroll-view scroll-y class="scrollPage">
    <view class="cu-card">
      <!-- 列表渲染面试测试题目 -->
      <block wx:for="{{exam_info}}" wx:key="{{item}}">
        <view class="cu-item bg-img shadow-blur" style="background-image:url(https://image.weilanwl.com/color2.0/plugin/wdh2236.jpg)" bindtap="toAq" data-src="{{item.examId}}">
          <view class="cardTitle">
             {{item.examName}}
          </view>
        </view>
      </block>
    </view>
  </scroll-view>
</view>
